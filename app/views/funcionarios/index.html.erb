<div class="row">
  <div class="inicio">
    <h1 class="help">Escolha o atendente para editar</h1>
  </div>
</div>
<div class="row">
  <div style="text-align: center; margin-top: 10px;">
    <div class="col-md-4 col-md-offset-4">
      <div class="col-md-10 col-md-offset-1">
      <% @atendentes.each do |a| %>
        <% f = a.funcionario %>
        <%= link_to f.nome, edit_funcionario_path(f.id), class: "btn btn-default btn-block", style: "text-overflow: ellipsis;"  %>
      <% end %>
      </div>
    </div>
  </div>
</div>

<div class="cancelar-reserva" style="margin-top: 10px; margin-right: 2.5%;">
  <%= link_to "Voltar", adm_path, class: 'cibelbutton' %>
</div>
